{
  "parser": "@babel/eslint-parser",
  "extends":[
    "eslint:recommended",
    "plugin:import/errors",
    "prettier"
  ],
  "env":{
    "browser": true,
    "node":true,
    "es6":true
  },
  "rules": {
    "no-debugger": 1,
    "no-console":1
  },
  "parserOptions":{
    "ecmaVersion": 2021,
    "sourceType":"module",
    "requireConfigFile": false
  }
}